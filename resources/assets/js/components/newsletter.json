{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "",
  "description": "",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 8,
      "maxLength": 80,
      "title": "Full Name"
    },
    "email": {
      "type": "string",
      "maxLength": 120,
      "title": "Email"
    },
    "source": {
      "type": "string",
      "maxLength": 120,
      "title": "Source",
      "description": "Ex. Using the NPM Search Engine"
    },
    "agree": {
      "type": "boolean",
      "title": "Agree",
      "description": "You agree to receive occasional updates and special offers for vue-json-schema updates.",
      "default": false,
      "attrs": {
        "type": "checkbox"
      }
    }
  },
  "additionalProperties": false,
  "required": ["name", "email"]
}