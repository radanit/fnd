{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "first_name": {
      "type": "string",
      "minLength": 8,
      "maxLength": 80,
      "title": "نام",
      "attrs": {
        "placeholder": "نام",
        "title": "trans('user.username')"
      }
    },
    "last_name": {
      "type": "string",
      "maxLength": 120,
      "title": "نام خانوادگی",
      "attrs": {
        "placeholder": "نام خانوادگی",
        "title": "trans('user.username')"
      }
    },
    "speciality_id": {
      "type": "string",
      "title": "تخصص",
      "enum": ["دندانپزشک", "ارتوپد"],
      "attrs": {
        "placeholder": "انتخاب تخصص",
        "title": "فیلد تخصص اجباری است"
      }
    },
    "agree": {
      "type": "boolean",
      "title": "Agree",
      "description": "You agree to receive occasional updates and special offers for vue-json-schema updates.",
      "default": false,
      "attrs": {
        "type": "checkbox"
      }
    }
  },
  "additionalProperties": false,
  "required": ["first_name", "last_name", "speciality_id"]
}